<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
<com.zhy.autolayout.AutoLinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_marginTop="40px">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/back"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/register"
        android:gravity="center"/>
</com.zhy.autolayout.AutoLinearLayout>

<com.zhy.autolayout.AutoLinearLayout
    android:layout_width="match_parent"
    android:layout_height="135px"
    android:orientation="horizontal">
<ImageView
    android:layout_width="44px"
    android:layout_height="44px"
    android:layout_marginLeft="20px"
    android:src="@mipmap/phone"
    android:layout_marginBottom="6px"
    android:layout_marginTop="70px"/>
    <View
        android:layout_width="8px"
        android:layout_height="0px">

    </View>
    <EditText
        android:layout_width="580px"
        android:layout_height="wrap_content"
        android:layout_marginTop="50px"
        android:textSize="24px"
        android:hint="请输入您的手机号"
        android:layout_marginBottom="14px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取验证码"
        android:textSize="20px"
        android:gravity="center"
        android:textColor="@color/fragmenthqyzmzt"
        android:background="@drawable/fragment_my_selector_ju"
        android:layout_marginBottom="10px"
        android:layout_marginTop="50px"
        android:layout_marginRight="20px"/>
</com.zhy.autolayout.AutoLinearLayout>
    <!--<View-->
        <!--android:layout_width="0px"-->
        <!--android:layout_height="54px">-->
    <!--</View>-->
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="92px"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="44px"
            android:layout_height="44px"
            android:layout_marginLeft="20px"
            android:src="@mipmap/yanzhengma"
            android:layout_marginBottom="6px"
            android:layout_marginTop="20px"
          />
        <View
            android:layout_width="8px"
            android:layout_height="0px">

        </View>
        <EditText
            android:layout_width="580px"
            android:layout_height="wrap_content"
            android:textSize="24px"
            android:hint="请输入验证码"
            android:layout_marginBottom="14px"/>

    </com.zhy.autolayout.AutoLinearLayout>
    <com.zhy.autolayout.AutoLinearLayout
    android:layout_width="match_parent"
    android:layout_height="92px"
    android:orientation="horizontal">
    <ImageView
        android:layout_width="44px"
        android:layout_height="44px"
        android:layout_marginLeft="20px"
        android:src="@mipmap/password"
        android:layout_marginBottom="6px"
        android:layout_marginTop="20px"
        />
    <View
        android:layout_width="8px"
        android:layout_height="0px">

    </View>
    <EditText
        android:layout_width="580px"
        android:layout_height="wrap_content"
        android:textSize="24px"
        android:hint="请设置您的密码"
        android:layout_marginBottom="14px"/>

</com.zhy.autolayout.AutoLinearLayout>
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="92px"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="44px"
            android:layout_height="44px"
            android:layout_marginLeft="20px"
            android:src="@mipmap/password"
            android:layout_marginBottom="6px"
            android:layout_marginTop="20px"
            />
        <View
            android:layout_width="8px"
            android:layout_height="0px">

        </View>
        <EditText
            android:layout_width="580px"
            android:layout_height="wrap_content"
            android:textSize="24px"
            android:hint="请确认您的密码"
            android:layout_marginBottom="14px"/>
    </com.zhy.autolayout.AutoLinearLayout>
    <Button
        android:id="@+id/fragement_my_login_atonce"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/fragmentmyatonceregister"
        android:textSize="28px"
        android:gravity="center"
        android:textColor="@color/textsize_my_btn"
        android:background="@drawable/my_button_login_selector"
        android:layout_marginRight="20px"
        android:layout_marginLeft="20px"
        android:layout_marginTop="70px"
        />
    <android.support.v7.widget.AppCompatCheckBox
        android:id="@+id/fragment_my_register_selector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="同意用户协议"
        android:layout_marginTop="20px"
        android:layout_marginLeft="20px"
        />
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="91px"
        android:orientation="horizontal">
        <View
            android:layout_width="244px"
            android:layout_height="1px"
            android:background="@color/fragmentmylineleft"
            android:layout_marginTop="85px"
            android:layout_marginLeft="10px">
        </View>
        <View
            android:layout_width="46px"
            android:layout_height="0px"
            android:layout_marginTop="70px">

        </View>
        <TextView
            android:id="@+id/fragment_my_dsf_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/fragmentmydsflogin"
            android:textSize="20px"
            android:textColor="@color/fragmentmydsflogin"
            android:layout_marginTop="70px"/>
        <View
            android:layout_width="46px"
            android:layout_height="0px"
            android:layout_marginTop="70px">

        </View>
        <View
            android:layout_width="244px"
            android:layout_height="1px"
            android:background="@color/fragmentmylineleft"
            android:layout_marginTop="85px"
            android:layout_marginRight="10px">
        </View>
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30px">
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/fragment_my_login_qq"
                android:layout_width="86px"
                android:layout_height="86px"
                android:src="@mipmap/qq"
                android:layout_marginLeft="234px"
                android:layout_marginTop="36px"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/fragment_my_login_qq"
                android:layout_marginLeft="240px"
                android:layout_marginTop="18px"
                android:textSize="20px"/>
        </com.zhy.autolayout.AutoLinearLayout>

        <View
            android:layout_width="80px"
            android:layout_height="0px">
        </View>
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/fragment_my_login_weixin"
                android:layout_width="86px"
                android:layout_height="86px"
                android:src="@mipmap/weixin"
                android:layout_marginRight="234px"
                android:layout_marginTop="36px"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/fragment_my_login_wexin"
                android:layout_marginRight="234px"
                android:layout_marginTop="18px"
                android:textSize="20px"/>
        </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>